<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/petition_edf2f6"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/rl_edit_personal_info"
            android:layout_width="match_parent"
            android:layout_height="@dimen/petition_47dp"
            android:layout_marginTop="@dimen/petition_7dp"
            android:background="@color/petition_ffffff">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="@dimen/petition_10dp"
                android:layout_height="@dimen/petition_10dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/petition_22dp"
                app:srcCompat="@drawable/petition_arrows" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="@dimen/petition_1dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="@dimen/petition_10dp"
                android:background="@color/petition_aeaeae" />

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="@dimen/petition_12dp"
                android:layout_height="@dimen/petition_22dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/petition_10dp"
                app:srcCompat="@drawable/petition_person" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/petition_10dp"
                android:layout_toEndOf="@+id/imageView18"
                android:layout_toRightOf="@+id/imageView18"
                android:text="@string/petition_personal_01"
                android:textColor="@color/petition_565656"
                android:textSize="@dimen/petition_12dp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_edit_phonenum"
            android:layout_width="match_parent"
            android:layout_height="@dimen/petition_47dp"
            android:background="@color/petition_ffffff">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="@dimen/petition_10dp"
                android:layout_height="@dimen/petition_10dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/petition_22dp"
                app:srcCompat="@drawable/petition_arrows" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="@dimen/petition_1dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="@dimen/petition_10dp"
                android:background="@color/petition_aeaeae" />

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="@dimen/petition_12dp"
                android:layout_height="@dimen/petition_22dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/petition_10dp"
                app:srcCompat="@drawable/petition_phone" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/petition_10dp"
                android:layout_toEndOf="@+id/imageView18"
                android:layout_toRightOf="@+id/imageView18"
                android:text="@string/petition_personal_02"
                android:textColor="@color/petition_565656"
                android:textSize="@dimen/petition_12dp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_edit_password"
            android:layout_width="match_parent"
            android:layout_height="@dimen/petition_47dp"
            android:background="@color/petition_ffffff">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="@dimen/petition_10dp"
                android:layout_height="@dimen/petition_10dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/petition_22dp"
                app:srcCompat="@drawable/petition_arrows" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="@dimen/petition_1dp"
                android:layout_alignParentBottom="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginLeft="@dimen/petition_10dp"
                android:background="@color/petition_aeaeae" />

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="@dimen/petition_12dp"
                android:layout_height="@dimen/petition_22dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/petition_10dp"
                app:srcCompat="@drawable/petition_password" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/petition_10dp"
                android:layout_toEndOf="@+id/imageView18"
                android:layout_toRightOf="@+id/imageView18"
                android:text="@string/petition_personal_03"
                android:textColor="@color/petition_565656"
                android:textSize="@dimen/petition_12dp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_exit_login"
            android:layout_width="match_parent"
            android:layout_height="@dimen/petition_44dp"
            android:layout_marginBottom="@dimen/petition_138dp"
            android:layout_marginLeft="@dimen/petition_10dp"
            android:layout_marginRight="@dimen/petition_10dp"
            android:layout_marginTop="@dimen/petition_57dp"
            android:background="@color/petition_0461bc">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="@string/petition_personal_04"
                android:textColor="@color/petition_ffffff"
                android:textSize="@dimen/petition_14dp" />
        </RelativeLayout>
    </LinearLayout>
</layout>