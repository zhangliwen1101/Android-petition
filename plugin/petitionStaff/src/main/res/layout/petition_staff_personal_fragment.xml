                        android:layout_marginLeft="@dimen/petition_staff_10dp"
                        android:layout_toEndOf="@+id/imageView13"
                        android:layout_toRightOf="@+id/imageView13"
                        android:text="@string/petition_staff_import_submission"
                        android:textColor="@color/petition_staff_565656"
                        android:textSize="@dimen/petition_staff_12dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/petition_staff_7dp"
                    android:background="@color/petition_staff_ffffff">

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_petition_submission01"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/petition_staff_47dp"
                    android:background="@color/petition_staff_ffffff">

                    <ImageView

                        android:layout_width="@dimen/petition_staff_10dp"
                        android:layout_height="@dimen/petition_staff_10dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="@dimen/petition_staff_22dp"
                        app:srcCompat="@drawable/petition_staff_arrows" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/petition_staff_1dp"
                        android:layout_alignParentBottom="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_marginLeft="@dimen/petition_staff_10dp"
                        android:background="@color/petition_staff_aeaeae" />

                    <ImageView
                        android:id="@+id/imageView17"
                        android:layout_width="@dimen/petition_staff_12dp"
                        android:layout_height="@dimen/petition_staff_22dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/petition_staff_10dp"
                        app:srcCompat="@drawable/petition_staff_qingbao" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/petition_staff_10dp"
                        android:layout_toEndOf="@+id/imageView17"
                        android:layout_toRightOf="@+id/imageView17"
                        android:text="@string/petition_staff_petition_submission"
                        android:textColor="@color/petition_staff_565656"
                        android:textSize="@dimen/petition_staff_12dp" />
                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/rl_exit_login"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/petition_staff_44dp"
                    android:layout_marginBottom="@dimen/petition_staff_138dp"
                    android:layout_marginLeft="@dimen/petition_staff_10dp"
                    android:layout_marginRight="@dimen/petition_staff_10dp"
                    android:layout_marginTop="@dimen/petition_staff_57dp"
                    android:background="@color/petition_staff_0461bc">

                    <TextView
                        android:id="@+id/textView6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:text="@string/petition_staff_login_out"
                        android:textColor="@color/petition_staff_ffffff"
                        android:textSize="@dimen/petition_staff_14dp" />
                </RelativeLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
